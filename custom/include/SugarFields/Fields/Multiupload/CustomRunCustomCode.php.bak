                    echo  'a : '.$date_last;
                }else if($file_timestamp < $date_last){
                    $date_last = $file_timestamp;
                    echo  'b : '.$date_last;
                }
            }
            if($date_last >= strtotime("-10 minutes")){
                $check = false;
            }else{
